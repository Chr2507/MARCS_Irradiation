#makeinput

> Irrad.dat
SECONDS=0;
cp models/t25g45.arciv  arcivaaa.dat
cp mxms7m31.input mxms7.input
./marcs

mv mxmodel.dat  models/t25g45ir31.dat
mv arcivaab.dat models/t25g45ir31.arciv
cp mxms7.input  models/t25g45ir31.input
cp Irrad.dat    models/t25g45ir31.irrad

rm spectrum.dat
rm fort.*
rm FLUX.DAT
rm arcivaaa.dat

hou=$((SECONDS/3600))
min=$(((SECONDS%3600)/60))
sec=$(((SECONDS%3600)%60))
echo  "  runmarcs runtime was: $hou hour, $min minutes & $sec seconds."
echo  "   " 


