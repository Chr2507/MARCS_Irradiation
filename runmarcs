#makeinput

> Irrad.dat
SECONDS=0;
cp models/t30g45.arciv  arcivaaa.dat
./marcs
mv mxmodel.dat models/t29g45irs.dat
mv arcivaab.dat models/t29g45irs.arciv
rm spectrum.dat
rm fort.*
rm FLUX.DAT
rm arcivaaa.dat

hou=$((SECONDS/3600))
min=$(((SECONDS%3600)/60))
sec=$(((SECONDS%3600)%60))
echo  "  runmarcs runtime was: $hou hour, $min minutes & $sec seconds."
echo  "   " 
